@model projecten.Models.BegIngenomenOpdrachtenModel
@{
    ViewBag.Title = "StageOpdracht wijzigen";
}

<h2>StageOpdracht wijzigen</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

    <fieldset>
        <legend>Registration Form</legend>
        <ol>
            <li>
                @Html.LabelFor(m => m.Omschrijving)
                @Html.TextAreaFor(m => m.Omschrijving)
            </li>
        </ol>
        <input class="linkbutton" type="submit" value="Wijzig" />
    </fieldset>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
