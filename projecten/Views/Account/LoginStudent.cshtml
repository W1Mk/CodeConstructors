@model projecten.Models.LoginStudentModel
@{
    ViewBag.Title = "LoginStudent";
    }
    <section id="LoginStudent">
<h2>log in als student</h2>
    @using (Html.BeginForm(new { ReturnUrl = ViewBag.ReturnUrl })) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Log in Form</legend>
        <ol>
            <li>
                @Html.LabelFor(m => m.Email)
                @Html.TextBoxFor(m => m.Email)
                @Html.ValidationMessageFor(m => m.Email)

                 @Html.LabelFor(m => m.Wachtwoord)
                @Html.PasswordFor(m => m.Wachtwoord)
                @Html.ValidationMessageFor(m => m.Wachtwoord)
                
            </li>
           
             
        </ol>
        <input type="submit" value="Log in" />
    </fieldset>
  
}
        </section>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
