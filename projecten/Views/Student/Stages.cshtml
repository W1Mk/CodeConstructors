@model IEnumerable<projecten.Models.Domain.StageOpdracht>

<script type="text/javascript">
    function Bekijk() {
        //get xmlhttprequest object
        var xhr = window.getXmlHttpRequest();

        //setup request
        xhr.open("GET", "/Student/Bekijk", true);

        //setup callback function
        xhr.onreadystatechange = function () {
            //readyState 4 : received answer from server
            if (xhr.readyState == 4 && xhr.status == 200)
                //add result to page
                $('#result').text(xhr.responseText);
        };

        //fire  request
        xhr.send(null);
    }
</script>

@section Scripts
{
    @Scripts.Render("~/bundles/jqueryval")
}
<h2>Stages</h2>
@using (Html.BeginForm("Stages", "Student"))
{
    <h5>Zoek op naam,semester, specialisatie of academiejaar</h5>
   <div id="buttons">zoekopdracht:<input type="text" name="zoekopdracht" style="width: 150px;" /> <input class="linkbutton" type="submit" value="Filter" /> </div>
  
}

<span id="message" style="float: right; margin-right: 20px; background-color: #333; color: #eee;"></span>

